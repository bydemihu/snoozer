{
  "manifest_version": 3,
  "name": "SnoozerAI",
  "description": "Satirical AI attention checker",
  "version": "1.0",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["script.js"]
  }],
  "action": {
    "default_popup": "test.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "options_page": "options.html"
}